{
  "name": "@olla/feature-flags-dashboard",
  "version": "1.0.1",
  "description": "Feature Flags Dashboard",
  "main": "index.js",
  "scripts": {
    "cleanup": "rm -rf dist && mkdir dist",
    "build": "npm run cleanup && webpack -p --progress",
    "start": "webpack-dev-server --progress --colors",
    "test:code": "typed-test 'source/**/*.test.ts'",
    "test:lint": "eslint --fix 'source/**/*.ts' 'source/**/*.tsx'",
    "test": "npm run test:code && npm run test:lint",
    "s3": "s3-deploy --gzip --cwd './dist' --region us-west-1 --bucket olla-feature-flag-dashboard './dist/**' --cache 604800",
    "cloudfront": "cf-invalidate -- E3QHWYYFNW522N '/*'",
    "production": "NODE_ENV=production BUILD_ENV=production npm run build && npm run s3 && npm run cloudfront",
    "slack": "../../tools/pingSlack/pingSlackCli.js --directory $PWD 'Feature Flags Dashboard'",
    "postversion": "npm run production && npm run slack"
  },
